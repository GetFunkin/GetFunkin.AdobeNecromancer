<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net5.0</TargetFramework>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Title>Tomat.GetFunkin.AdobeNecromancer</Title>
    <Authors>Tomat</Authors>
    <Description>Simple library for processing Sparrow-formatted and SpriteSheetPacker-formatted atlases. Called "AdobeNecromancer" because it was originally made to parse the Sparrow atlases generated by Adobe Animate, and raising the dead is a form of animation.</Description>
    <Copyright>MIT</Copyright>
    <PackageProjectUrl>https://github.com/GetFunkin/GetFunkin.AdobeNecromancer</PackageProjectUrl>
    <PackageLicenseUrl>https://github.com/GetFunkin/GetFunkin.AdobeNecromancer/blob/master/LICENSE.md</PackageLicenseUrl>
    <RepositoryUrl>https://github.com/GetFunkin/GetFunkin.AdobeNecromancer</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
    <PackageTags>adobe, adobeanimate, atlas</PackageTags>
    <PackageVersion>1.0.1</PackageVersion>
    <AssemblyVersion>1.0.1</AssemblyVersion>
    <FileVersion>1.0.1</FileVersion>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="System.Drawing.Common" Version="5.0.2" />
  </ItemGroup>

  <ItemGroup>
    <None Update="DebugFiles\logoBumpin.png">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </None>
    <None Update="DebugFiles\logoBumpin.xml">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </None>
  </ItemGroup>

</Project>
